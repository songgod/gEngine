<UserControl x:Class="GPTDxWPFRibbonApplication1.Controls.WellLocationControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GPTDxWPFRibbonApplication1.Controls"
             xmlns:gv="clr-namespace:gEngine.View;assembly=gEngine.View"
             xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
             xmlns:em="clr-namespace:gEngine.Manipulator;assembly=gEngine.Manipulator"
             Loaded="Window_Loaded"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Border Background="White" BorderBrush="Black" BorderThickness="2">
            <gv:MapControl x:Name="mc">
                <i:Interaction.Behaviors>
                    <em:MapControlAdonDectZoomPan/>
                </i:Interaction.Behaviors>
            </gv:MapControl>
        </Border>
    </Grid>
</UserControl>
