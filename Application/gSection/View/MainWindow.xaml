<dxr:DXRibbonWindow
        x:Uid="dxr:DXRibbonWindow_1" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
        xmlns:dxb="http://schemas.devexpress.com/winfx/2008/xaml/bars"
        xmlns:dxr="http://schemas.devexpress.com/winfx/2008/xaml/ribbon"
        xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
        xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        xmlns:local="clr-namespace:gSection"
        xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
        xmlns:prjcontrols="clr-namespace:gEngine.Project.Controls;assembly=gEngine.Project"
        xmlns:prjcmds="clr-namespace:gEngine.Commands;assembly=gEngine.Commands"
        xmlns:cvt="clr-namespace:gSection.Converters"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" x:Class="gSection.View.MainWindow"
        Title="MainWindow" Height="725" Width="1452"
    x:Name="dxrWindow">
    <dxr:DXRibbonWindow.Resources>
        <ResourceDictionary x:Uid="ResourceDictionary_1">
            <cvt:ObjectsEqualityConverter x:Uid="cvt:ObjectsEqualityConverter_1" x:Key="objectsEqualityConverter"/>
            <x:Array x:Uid="x:Array_1" x:Key="gallerypic" Type="{x:Type sys:String}">
                <sys:String x:Uid="sys:String_1">/Images/Clipart/caCompClient.png</sys:String>
                <sys:String x:Uid="sys:String_2">/Images/Clipart/caCompClientEnabled.png</sys:String>
                <sys:String x:Uid="sys:String_3">/Images/Clipart/caDatabaseBlue.png</sys:String>
                <sys:String x:Uid="sys:String_4">/Images/Clipart/caDataBaseDisabled.png</sys:String>
                <sys:String x:Uid="sys:String_5">/Images/Clipart/caDataBaseGreen.png</sys:String>
                <sys:String x:Uid="sys:String_6">/Images/Clipart/caDataBaseViolet.png</sys:String>
                <sys:String x:Uid="sys:String_7">/Images/Clipart/caInet.png</sys:String>
                <sys:String x:Uid="sys:String_8">/Images/Clipart/caInetSearch.png</sys:String>
                <sys:String x:Uid="sys:String_9">/Images/Clipart/caModem.png</sys:String>
                <sys:String x:Uid="sys:String_10">/Images/Clipart/caModemEnabled.png</sys:String>
                <sys:String x:Uid="sys:String_11">/Images/Clipart/caNetCard.png</sys:String>
                <sys:String x:Uid="sys:String_12">/Images/Clipart/caNetwork.png</sys:String>
                <sys:String x:Uid="sys:String_13">/Images/Clipart/caNetworkEnabled.png</sys:String>
                <sys:String x:Uid="sys:String_14">/Images/Clipart/caServer.png</sys:String>
                <sys:String x:Uid="sys:String_15">/Images/Clipart/caServerEnabled.png</sys:String>
                <sys:String x:Uid="sys:String_16">/Images/Clipart/caWebCam.png</sys:String>
            </x:Array>
            <dx:IEnumerableDataSource x:Uid="dx:IEnumerableDataSource_1" x:Key="IEnumerableDataSource">
                <dx:DesignDataManager.DesignData>
                    <dx:DesignDataSettings x:Uid="dx:DesignDataSettings_1" DataObjectType="{x:Type local:App}" RowCount="5"/>
                </dx:DesignDataManager.DesignData>
            </dx:IEnumerableDataSource>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Uid="ResourceDictionary_2" Source="/gSection;component/View/BackstageViewPanes.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </dxr:DXRibbonWindow.Resources>
    <dxr:DXRibbonWindow.InputBindings>
        <KeyBinding x:Uid="KeyBinding_1" Gesture="Ctrl+Z" Command="{x:Static prjcmds:StartCommands.UndoCommand}" CommandTarget="{Binding ElementName=prjctrl}"/>
        <KeyBinding x:Uid="KeyBinding_2" Gesture="Ctrl+Y" Command="{x:Static prjcmds:StartCommands.RedoCommand}" CommandTarget="{Binding  ElementName=prjctrl}"/>
    </dxr:DXRibbonWindow.InputBindings>
    <Grid x:Uid="Grid_1">
        <Grid.RowDefinitions>
            <RowDefinition x:Uid="RowDefinition_1" Height="Auto"/>
            <RowDefinition x:Uid="RowDefinition_2" Height="*"/>
            <RowDefinition x:Uid="RowDefinition_3" Height="Auto"/>
        </Grid.RowDefinitions>
        <dxr:RibbonControl  x:Uid="ribbonControl" x:Name="ribbonControl" Grid.Row="0" RibbonStyle="Office2007" ShowApplicationButton="True" PageCategoryAlignment="Left" MinimizationButtonVisibility="Auto">
            <dxr:RibbonControl.Background>
                <ImageBrush x:Uid="ImageBrush_1"/>
            </dxr:RibbonControl.Background>
            <dxr:RibbonControl.PageHeaderItems>
                <dxb:BarButtonItem x:Uid="dxb:BarButtonItem_1" Content="BarButtonItem"/>
                <dxb:BarItemLink x:Uid="dxb:BarItemLink_1" BarItemName="ribbonStyleItem" RibbonStyle="SmallWithText"/>
            </dxr:RibbonControl.PageHeaderItems>
            <dxr:RibbonControl.ApplicationMenu x:Uid="a">
                <dxr:BackstageViewControl x:Name="BackstageViewControl_1" x:Uid="dxr:BackstageViewControl_1">
                    <dxr:BackstageButtonItem x:Uid="dxr:BackstageButtonItem_1" Content="New" Glyph="{dx:DXImage Image=New_16x16.png}" MinWidth="150"/>
                    <dxr:BackstageTabItem x:Uid="dxr:BackstageTabItem_OpenProject" dxb:MergingProperties.AllowMerging="True"  dxb:MergingProperties.ElementMergingBehavior="InternalWithInternal" dxb:BarNameScope.IsScopeOwner="True">
                        <dxr:BackstageTabItem.Content>
                            <Grid x:Uid="Grid_2">
                                <Grid.RenderTransform>
                                    <TranslateTransform x:Uid="TranslateTransform_1" X="-19"></TranslateTransform>
                                </Grid.RenderTransform>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition x:Uid="ColumnDefinition_1" Width="Auto"></ColumnDefinition>
                                    <ColumnDefinition x:Uid="ColumnDefinition_2" Width="*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Image x:Uid="Image_1" Source="../Images/Small/FileOpen2_16x16.png" Margin="0 0 4 0"></Image>
                                <StackPanel x:Uid="StackPanel_1" VerticalAlignment="Center" Grid.Column="1">
                                    <TextBlock x:Uid="TextBlock_1" Text="打开工区"></TextBlock>
                                </StackPanel>
                            </Grid>
                        </dxr:BackstageTabItem.Content>
                        <dxr:BackstageTabItem.ControlPane>
                            <ContentControl  x:Uid="ContentControl_4" Template="{StaticResource ProjectTemplate}"></ContentControl>
                        </dxr:BackstageTabItem.ControlPane>
                    </dxr:BackstageTabItem>
                    <dxr:BackstageTabItem  x:Uid="dxr:BackstageTabItem_OpenMap"  dxb:MergingProperties.AllowMerging="True" MinWidth="150" dxb:MergingProperties.ElementMergingBehavior="InternalWithInternal" dxb:BarNameScope.IsScopeOwner="True">
                        <dxr:BackstageTabItem.Content>
                            <Grid x:Uid="Grid_3">
                                <Grid.RenderTransform>
                                    <TranslateTransform x:Uid="TranslateTransform_2" X="-19"></TranslateTransform>
                                </Grid.RenderTransform>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition x:Uid="ColumnDefinition_3" Width="Auto"></ColumnDefinition>
                                    <ColumnDefinition x:Uid="ColumnDefinition_4" Width="*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Image x:Uid="Image_2" Source="../Images/Small/FileNew_16x16.png" Margin="0 0 4 0"></Image>
                                <StackPanel x:Uid="StackPanel_2" VerticalAlignment="Center" Grid.Column="1">
                                    <TextBlock x:Uid="TextBlock_2" Text="打开图件"></TextBlock>
                                </StackPanel>
                            </Grid>
                        </dxr:BackstageTabItem.Content>
                        <dxr:BackstageTabItem.ControlPane>
                            <ContentControl  x:Uid="ContentControl_5" Template="{StaticResource MapTemplate}"></ContentControl>
                        </dxr:BackstageTabItem.ControlPane>
                    </dxr:BackstageTabItem>
                    <dxr:BackstageButtonItem  x:Uid="dxr:BackstageButtonItem_4" Content="保存" Glyph="{dx:DXImage Image=Save_16x16.png}" MinWidth="150" Command="{x:Static prjcmds:ProjectCommands.SaveCommand}" CommandTarget="{Binding ElementName=prjctrl}"/>
                    <dxr:BackstageButtonItem  x:Uid="dxr:BackstageButtonItem_5" Content="另存为" Glyph="{dx:DXImage Image=SaveAs_16x16.png}" MinWidth="150" Command="{x:Static prjcmds:ProjectCommands.SaveAsCommand}" CommandTarget="{Binding ElementName=prjctrl}"/>
                    <dxr:BackstageTabItem x:Uid="dxr:BackstageTabItem_1" dxb:MergingProperties.AllowMerging="True" Content="Recent" dxb:MergingProperties.ElementMergingBehavior="InternalWithInternal" dxb:BarNameScope.IsScopeOwner="True">
                    </dxr:BackstageTabItem>
                    <dxr:BackstageTabItem x:Uid="dxr:BackstageTabItem_2" dxb:MergingProperties.AllowMerging="True" Content="Options" dxb:MergingProperties.ElementMergingBehavior="InternalWithInternal" dxb:BarNameScope.IsScopeOwner="True">
                        <!--<view:OptionPanel/>-->
                    </dxr:BackstageTabItem>
                    <dxr:BackstageTabItem x:Uid="dxr:BackstageTabItem_3" dxb:MergingProperties.AllowMerging="True" Content="Help" dxb:MergingProperties.ElementMergingBehavior="InternalWithInternal" dxb:BarNameScope.IsScopeOwner="True"/>
                    <dxr:BackstageButtonItem x:Uid="dxr:BackstageButtonItem_3" Content="Close" Glyph="{dx:DXImage Image=Close_16x16.png}"/>
                </dxr:BackstageViewControl>
            </dxr:RibbonControl.ApplicationMenu>
            <dxr:RibbonControl.ToolbarItems>
                <dxr:RibbonSpacingSelectorItemLink x:Uid="dxr:RibbonSpacingSelectorItemLink_1" BarItemName="spacingSelector"/>
                <dxb:BarButtonItemLink x:Uid="dxb:BarButtonItemLink_1" BarItemName="newItem"/>
                <dxb:BarButtonItemLink x:Uid="dxb:BarButtonItemLink_2" BarItemName="barButtonItem1"/>
            </dxr:RibbonControl.ToolbarItems>
            <dxr:RibbonDefaultPageCategory x:Uid="dxr:RibbonDefaultPageCategory_1">
                <dxr:RibbonPage x:Uid="pageStart" x:Name="pageStart" Caption="开始">
                    <dxr:RibbonPageGroup x:Uid="dxr:RibbonPageGroup_1" Caption="编辑">
                        <dxb:BarButtonItem x:Uid="dxb:BarButtonItem_2" Content="撤销"  Command="{x:Static prjcmds:StartCommands.UndoCommand}" CommandTarget="{Binding}" LargeGlyph="{dx:DXImage Image=Undo_32x32.png}" Glyph="{dx:DXImage Image=Undo_16x16.png}"/>
                        <dxb:BarButtonItem x:Uid="dxb:BarButtonItem_3" Content="重做"  Command="{x:Static prjcmds:StartCommands.RedoCommand}" CommandTarget="{Binding}" LargeGlyph="{dx:DXImage Image=Redo_32x32.png}" Glyph="{dx:DXImage Image=Redo_16x16.png}"/>
                        <dxb:BarButtonItem x:Uid="dxb:BarButtonItem_4" Content="删除" LargeGlyph="{dx:DXImage Image=Delete_32x32.png}" Glyph="{dx:DXImage Image=Delete_16x16.png}"/>
                        <dxb:BarButtonItem x:Uid="dxb:BarButtonItem_5" Content="清理内存" LargeGlyph="/gSection;component/Images/GDS/清理内存.png" Glyph="/gSection;component/Images/GDS/清理内存.png"/>
                    </dxr:RibbonPageGroup>
                    <dxr:RibbonPageGroup x:Uid="dxr:RibbonPageGroup_2" Caption="打印">
                        <dxb:BarButtonItem x:Uid="dxb:BarButtonItem_6" Content="打印" LargeGlyph="{dx:DXImage Image=Printer_32x32.png}" Glyph="{dx:DXImage Image=Printer_16x16.png}"/>
                        <dxb:BarButtonItem x:Uid="dxb:BarButtonItem_7" Content="设置范围" LargeGlyph="{dx:DXImage Image=RangeArea_32x32.png}" Glyph="{dx:DXImage Image=RangeArea_16x16.png}"/>
                    </dxr:RibbonPageGroup>
                    <dxr:RibbonPageGroup x:Uid="dxr:RibbonPageGroup_29" Caption="常用功能">
                        <dxb:BarButtonItem x:Uid="dxb:BarButtonItem_39" Content="设置数据源" Command="{x:Static prjcmds:ProjectCommands.OpenDBSourceCommand}" CommandTarget="{Binding ElementName=prjctrl}" LargeGlyph="pack://application:,,,/gSection;component/Images/GDS/数据源32.png" Glyph="pack://application:,,,/gSection;component/Images/GDS/数据源16.png"></dxb:BarButtonItem>
                        <dxb:BarButtonItem  x:Uid="dxb:BarButtonItem_42" x:Name="bbiSelect" Content="选择" 
                                            Command="{x:Static prjcmds:StartCommands.SelectObjectCommand}" CommandTarget="{Binding ElementName=prjctrl}"
                                            LargeGlyph="{dx:DXImage Image=SelectTool_32x32.png}" Glyph="{dx:DXImage Image=SelectTool_16x16.png}" RibbonStyle="Large"/>
                        <dxb:BarButtonItem x:Uid="dxb:BarButtonItem_46" Content="输出PNG"  Command="{x:Static prjcmds:BasicCommands.PrintPngCommand}" 
                          CommandTarget="{Binding ElementName=prjctrl}"
                                          LargeGlyph="pack://application:,,,/gSection;component/Images/Clipart/caNetCard.png" Glyph="pack://application:,,,/gSection;component/Images/Clipart/caNetCard.png" RibbonStyle="Large"/>
                        <dxb:BarButtonItem x:Uid="dxb:BarButtonItem_50" Content="打开平面图" Command="{x:Static prjcmds:PlaneCommands.NewPlaneMapCommand}"
                                           CommandTarget="{Binding ElementName=prjctrl}" Glyph="{dx:DXImage Image=Open2_16x16.png}" LargeGlyph="{dx:DXImage Image=Open2_32x32.png}"/>
                        <dxb:BarButtonItem x:Uid="dxb:BarButtonItem_51" Content="新建剖面图" Command="{x:Static prjcmds:SectionCommands.NewSectionMapCommand}"
                            CommandTarget="{Binding ElementName=prjctrl }"
                            LargeGlyph="{dx:DXImage Image=BOFolder_32x32.png}" Glyph="{dx:DXImage Image=BOFolder_16x16.png}"/>
                    </dxr:RibbonPageGroup>
                </dxr:RibbonPage>
                <dxr:RibbonPage x:Uid="pageRef" x:Name="pageRef" Caption="参考">
                    <dxr:RibbonPageGroup x:Uid="dxr:RibbonPageGroup_4" Caption="平面信息">
                        <dxb:BarButtonItem x:Uid="dxb:BarButtonItem_8" Content="打开平面图" Command="{x:Static prjcmds:PlaneCommands.NewPlaneMapCommand}" CommandTarget="{Binding ElementName=prjctrl}" Glyph="{dx:DXImage Image=Open2_16x16.png}" LargeGlyph="{dx:DXImage Image=Open2_32x32.png}"/>
                    </dxr:RibbonPageGroup>
                    <dxr:RibbonPageGroup x:Uid="dxr:RibbonPageGroup_5" Caption="地震信息">
                        <dxb:BarCheckItem x:Uid="dxb:BarCheckItem_1" Content="BarCheckItem"/>
                        <dxb:BarCheckItem x:Uid="dxb:BarCheckItem_2" Content="BarCheckItem"/>
                    </dxr:RibbonPageGroup>
                </dxr:RibbonPage>
                <dxr:RibbonPage x:Uid="pSettings" x:Name="pSettings" Caption="Setting">
                    <dxr:RibbonPageGroup x:Uid="dxr:RibbonPageGroup_16" Caption="Theme" ShowCaptionButton="False" SmallGlyph="{dx:DXImage Image=Palette_16x16.png}">
                        <dxr:RibbonGalleryBarItem x:Uid="dxr:RibbonGalleryBarItem_2" Content="Theme" Glyph="{dx:DXImage Image=Colors_16x16.png}" LargeGlyph="{dx:DXImage Image=Colors_32x32.png}">
                            <dxr:RibbonGalleryBarItem.DropDownMenuItems>
                                <dxr:RibbonSpacingSelectorItem x:Uid="spacingSelector" x:Name="spacingSelector" Content="Space mode"/>
                                <dxb:BarSplitButtonItem x:Uid="ribbonStyleItem" x:Name="ribbonStyleItem" ActAsDropDown="True" Content="Ribbon Style" ItemClickBehaviour="CloseAllPopups">
                                    <dxb:BarSplitButtonItem.PopupControl>
                                        <dxb:PopupControlContainer x:Uid="dxb:PopupControlContainer_1">
                                            <dxb:GalleryControl x:Uid="dxb:GalleryControl_1">
                                                <dxb:Gallery x:Uid="dxb:Gallery_2" AllowFilter="False" ItemDescriptionHorizontalAlignment="Left" AutoHideGallery="True" ItemCheckMode="Single">
                                                    <dxb:GalleryItemGroup x:Uid="dxb:GalleryItemGroup_1" Caption="Classic">
                                                        <dxb:GalleryItem x:Uid="dxb:GalleryItem_1" Caption="Office2007" Description="MS Office 2007 style Ribbon" IsChecked="{Binding ElementName=ribbonControl, Path=RibbonStyle, Mode=TwoWay, Converter={StaticResource objectsEqualityConverter}, ConverterParameter=Office2007}" Glyph="/Images/RibbonStyle/Office2007.png"/>
                                                        <dxb:GalleryItem x:Uid="dxb:GalleryItem_2" Caption="Office2010" Description="MS Office 2010 style Ribbon" IsChecked="{Binding ElementName=ribbonControl, Path=RibbonStyle, Mode=TwoWay, Converter={StaticResource objectsEqualityConverter}, ConverterParameter=Office2010}" Glyph="/Images/RibbonStyle/Office2010.png"/>
                                                    </dxb:GalleryItemGroup>
                                                    <dxb:GalleryItemGroup x:Uid="dxb:GalleryItemGroup_2" Caption="Tablet">
                                                        <dxb:GalleryItem x:Uid="dxb:GalleryItem_3" Caption="TabletOffice" Description="Emulates the UI found in MS Office for iPad" IsChecked="{Binding ElementName=ribbonControl, Path=RibbonStyle, Mode=TwoWay, Converter={StaticResource objectsEqualityConverter}, ConverterParameter=TabletOffice}" Glyph="/Images/RibbonStyle/TabletOffice.png"/>
                                                        <dxb:GalleryItem x:Uid="dxb:GalleryItem_4" Caption="OfficeSlim" Description="The Ribbon style inspired by the ribbon UI introduced in Microsoft &quot;Office Universal&quot; apps." IsChecked="{Binding ElementName=ribbonControl, Path=RibbonStyle, Mode=TwoWay, Converter={StaticResource objectsEqualityConverter}, ConverterParameter=OfficeSlim}" Glyph="/Images/RibbonStyle/OfficeSlim.png"/>
                                                    </dxb:GalleryItemGroup>
                                                </dxb:Gallery>
                                            </dxb:GalleryControl>
                                        </dxb:PopupControlContainer>
                                    </dxb:BarSplitButtonItem.PopupControl>
                                </dxb:BarSplitButtonItem>
                            </dxr:RibbonGalleryBarItem.DropDownMenuItems>
                            <dxmvvm:Interaction.Behaviors>
                                <dxr:RibbonGalleryItemThemeSelectorBehavior x:Uid="dxr:RibbonGalleryItemThemeSelectorBehavior_1"/>
                            </dxmvvm:Interaction.Behaviors>
                        </dxr:RibbonGalleryBarItem>
                    </dxr:RibbonPageGroup>
                </dxr:RibbonPage>
                <dxr:RibbonPage x:Uid="dxr:RibbonPage_4" Caption="修饰">
                    <dxr:RibbonPageGroup x:Uid="dxr:RibbonPageGroup_17" Caption="通用图层">
                        <dxb:BarButtonItem x:Uid="dxb:BarButtonItem_27" Content="比例尺"  Command="{x:Static prjcmds:BasicCommands.NewScaleRuleCommand}" 
                           CommandTarget="{Binding Path=ProjectControl, RelativeSource={RelativeSource AncestorType=dxr:DXRibbonWindow}}"/>
                        <dxb:BarButtonItem x:Uid="dxb:BarButtonItem_29" Content="添加直线"  Command="{x:Static prjcmds:BasicCommands.NewLineCommand}" 
                           CommandTarget="{Binding Path=ProjectControl, RelativeSource={RelativeSource AncestorType=dxr:DXRibbonWindow}}"/>
                        <dxb:BarButtonItem x:Uid="dxb:BarButtonItem_30" Content="添加面"  Command="{x:Static prjcmds:BasicCommands.NewBoundaryCommand}" 
                           CommandTarget="{Binding Path=ProjectControl, RelativeSource={RelativeSource AncestorType=dxr:DXRibbonWindow}}"/>
                        <dxb:BarButtonItem x:Uid="dxb:BarButtonItem_45" Content="添加矩形"  Command="{x:Static prjcmds:BasicCommands.NewRectCommand}" 
                           CommandTarget="{Binding Path=ProjectControl, RelativeSource={RelativeSource AncestorType=dxr:DXRibbonWindow}}"/>
                        <dxb:BarButtonItem  x:Uid="dxb:BarButtonItem_49" Content="添加折线"  Command="{x:Static prjcmds:BasicCommands.NewPolyLineCommand}" 
                           CommandTarget="{Binding Path=ProjectControl, RelativeSource={RelativeSource AncestorType=dxr:DXRibbonWindow}}"/>
                        <dxb:BarButtonItem  x:Uid="dxb:BarButtonItem_47" Content="添加曲线"  Command="{x:Static prjcmds:BasicCommands.NewBezierLineCommand}" 
                           CommandTarget="{Binding Path=ProjectControl, RelativeSource={RelativeSource AncestorType=dxr:DXRibbonWindow}}"/>
                        <dxb:BarButtonItem   x:Uid="dxb:BarButtonItem_48" Content="添加指北针"  Command="{x:Static prjcmds:BasicCommands.NewCompressCommand}" 
                           CommandTarget="{Binding Path=ProjectControl, RelativeSource={RelativeSource AncestorType=dxr:DXRibbonWindow}}"/>
                    </dxr:RibbonPageGroup>
                </dxr:RibbonPage>
                <dxr:RibbonPage x:Uid="dxr:RibbonPage_5" Caption="视图">
                    <dxr:RibbonPageGroup x:Uid="dxr:RibbonPageGroup_18" Caption="">
                        <dxb:BarButtonItem x:Uid="dxb:BarButtonItem_28" Content="全图显示" Command="{x:Static prjcmds:OperateViewCommands.FullViewCommand}" CommandTarget="{Binding ElementName=prjctrl}" LargeGlyph="pack://application:,,,/gSection;component/Images/GDS/20150310095307390_easyicon_net_32.ico" Glyph="pack://application:,,,/gSection;component/Images/GDS/20150310095307390_easyicon_net_32.ico"/>
                             <!--MouseLeftButtonDown="bbiSelect_MouseLeftButtonDown"-->
                    </dxr:RibbonPageGroup>
                    <dxr:RibbonPageGroup x:Uid="dxr:RibbonPageGroup_19" Caption="视图缩放">
                        <dxb:BarButtonItem x:Uid="dxb:BarButtonItem_43" Content="视图放大" LargeGlyph="pack://application:,,,/gSection;component/Images/GDS/20150707101248177_easyicon_net_64.ico"/>
                        <dxb:BarButtonItem x:Uid="dxb:BarButtonItem_44" Content="视图缩小" LargeGlyph="pack://application:,,,/gSection;component/Images/GDS/20150707101252119_easyicon_net_64.ico" Glyph="pack://application:,,,/gSection;component/Images/GDS/20150707101252119_easyicon_net_64.ico" RibbonStyle="Large"/>
                        <dxb:BarButtonItem x:Uid="dxb:BarButtonItem_31" Content="局部放大" LargeGlyph="pack://application:,,,/gSection;component/Images/GDS/20150707101236441_easyicon_net_64.ico" Glyph="pack://application:,,,/gSection;component/Images/GDS/20150707101236441_easyicon_net_64.ico" RibbonStyle="Large"/>
                        <dxb:BarButtonItem x:Uid="dxb:BarButtonItem_32" LargeGlyph="pack://application:,,,/gSection;component/Images/GDS/20150707101244900_easyicon_net_64.ico" Glyph="pack://application:,,,/gSection;component/Images/GDS/20150707101244900_easyicon_net_64.ico" Content="恢复试图" RibbonStyle="Large"/>
                        <dxb:BarButtonItem x:Uid="dxb:BarButtonItem_33" Content="移动" LargeGlyph="pack://application:,,,/gSection;component/Images/GDS/20150323015210751_easyicon_net_64.ico" Glyph="pack://application:,,,/gSection;component/Images/GDS/20150323015210751_easyicon_net_64.ico" RibbonStyle="Large"/>
                        <dxb:BarButtonItem x:Uid="dxb:BarButtonItem_34" Content="对象居中" LargeGlyph="pack://application:,,,/gSection;component/Images/GDS/20150707101222971_easyicon_net_64.ico" Glyph="pack://application:,,,/gSection;component/Images/GDS/20150707101222971_easyicon_net_64.ico" RibbonStyle="Large"/>
                    </dxr:RibbonPageGroup>
                    <dxr:RibbonPageGroup x:Uid="dxr:RibbonPageGroup_20" Caption="视图比例">
                        <dxb:BarEditItem x:Uid="dxb:BarEditItem_13" Content="显示比例（%）" EditWidth="100" EditValue="100">
                            <dxb:BarEditItem.EditSettings>
                                <dxe:ComboBoxEditSettings x:Uid="dxe:ComboBoxEditSettings_1"/>
                            </dxb:BarEditItem.EditSettings>
                        </dxb:BarEditItem>
                        <dxb:BarEditItem x:Uid="dxb:BarEditItem_14" Content="横向比例" EditWidth="100" EditValue="1:1000">
                            <dxb:BarEditItem.EditSettings>
                                <dxe:ComboBoxEditSettings x:Uid="dxe:ComboBoxEditSettings_2"/>
                            </dxb:BarEditItem.EditSettings>
                        </dxb:BarEditItem>
                        <dxb:BarEditItem x:Uid="dxb:BarEditItem_15" Content="纵向比例" EditWidth="100" EditValue="1:500">
                            <dxb:BarEditItem.EditSettings>
                                <dxe:ComboBoxEditSettings x:Uid="dxe:ComboBoxEditSettings_3"/>
                            </dxb:BarEditItem.EditSettings>
                        </dxb:BarEditItem>
                        <dxb:BarItemSeparator x:Uid="dxb:BarItemSeparator_4"/>
                    </dxr:RibbonPageGroup>
                    <dxr:RibbonPageGroup x:Uid="dxr:RibbonPageGroup_21" Caption="画布大小">
                        <dxb:BarEditItem x:Uid="dxb:BarEditItem_16" Content="左边（%）" EditWidth="50" EditValue="5">
                            <dxb:BarEditItem.EditSettings>
                                <dxe:TextEditSettings x:Uid="dxe:TextEditSettings_6"/>
