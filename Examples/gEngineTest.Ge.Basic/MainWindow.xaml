<Window x:Class="gEngineTest.Ge.Basic.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:gb="clr-namespace:gEngine.Graph.Ge.Basic;assembly=gEngine.Graph.Ge.Basic"
        xmlns:local="clr-namespace:gEngineTest.Ge.Basic"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Window.Resources>
        <gb:Rect x:Key="rect" Left="100" Top="100" Width="100" Height="100"/>
        <gb:Line x:Key="line" Start="20,20" End="40,40"/>
        <gb:PolyLine x:Key="polyline" Points="20,20,30,50,60,30"/>
        <gb:BeizerLine x:Key="bezierline" Points="50,50,70,30,60,100,30,80"/>
    </Window.Resources>
    <Grid>
        <ContentControl Content="{StaticResource rect}"/>
        <ContentControl Content="{StaticResource line}"/>
        <ContentControl Content="{StaticResource polyline}"/>
        <ContentControl Content="{StaticResource bezierline}"/>
    </Grid>
</Window>
