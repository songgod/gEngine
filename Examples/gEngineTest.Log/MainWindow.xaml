<Window x:Class="gEngineTest.Log.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:gEngineTest.Log"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <Button Content="LogError" x:Name="LogError" Click="LogError_Click"/>
        <Button Content="LogWarning" x:Name="LogWarning" Click="LogWarning_Click"/>
        <Button Content="LogInfo" x:Name="LogInfo" Click="LogInfo_Click"/>
        <Button Content="Crush" x:Name="Crush" Click="Crush_Click"/>
    </StackPanel>
</Window>
