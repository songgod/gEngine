<ResourceDictionary x:Class="gEngine.View.Ge.Basic.CompressDict"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:gb="clr-namespace:gEngine.Graph.Ge.Basic;assembly=gEngine.Graph.Ge.Basic"
             xmlns:local="clr-namespace:gEngine.View.Ge.Basic"
             mc:Ignorable="d">
    <DataTemplate DataType="{x:Type gb:Compress}">
        <StackPanel>
            <Path Data="{Binding CompressData}"
              RenderTransformOrigin="0.5,0.5"
              Fill="Black">
                <Path.RenderTransform>
                    <TransformGroup>
                        <TranslateTransform  />
                        <ScaleTransform />
                        <RotateTransform Angle="{Binding RotateAngle}" />
                    </TransformGroup>
                </Path.RenderTransform>
            </Path>
        </StackPanel>
    </DataTemplate>
</ResourceDictionary>
