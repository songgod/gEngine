    <DataTemplate DataType="{x:Type gb:Fill}">
        <gv:FillControl FillStyle="{Binding FillStyle}">
            <gv:FillControl.Resources>
                <local:Ponits2PolylinePathGeometryConverter x:Key="p2plpgc"/>
            </gv:FillControl.Resources>
            <gv:FillControl.Data>
                <Binding Path="Points" Converter="{StaticResource p2plpgc}"/>
            </gv:FillControl.Data>
        </gv:FillControl>
    </DataTemplate>
    <DataTemplate DataType="{x:Type gb:Rect}">
        <gv:FillControl FillStyle="{Binding FillStyle}">
            <gv:FillControl.Resources>
                <local:Points2PolyBezierPathGeometryConverter x:Key="p2pbpgc"/>
            </gv:FillControl.Resources>
            <gv:FillControl.Data>
                <Binding Path="Points" Converter="{StaticResource p2pbpgc}"/>
            </gv:FillControl.Data>
        </gv:FillControl>
    </DataTemplate>
</ResourceDictionary>
